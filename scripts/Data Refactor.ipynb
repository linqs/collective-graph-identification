    "\n",
    "    string_similarity = qgram.distance(email_1, email_2)\n",
    "\n",
    "    row_dict = {'email':pair[0], 'other_email':pair[1], 'qgram_sim':string_similarity}\n",
    "    row_list.append(row_dict)\n",
    "    \n",
    "sim_email = pd.concat([sim_email, pd.DataFrame(row_list)])"
